{
  "name": "node-ipc-jsonrpc",
  "version": "0.1.0",
  "description": "Modern TypeScript client for JSON-RPC over IPC (Unix sockets/Named Pipes) - designed for Go servers",
  "keywords": [
    "ipc",
    "jsonrpc",
    "json-rpc",
    "unix-socket",
    "named-pipe",
    "cross-platform",
    "go",
    "typescript",
    "vscode",
    "electron"
  ],
  "author": "gnana997",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gnana997/ipc-jsonrpc.git",
    "directory": "node"
  },
  "bugs": {
    "url": "https://github.com/gnana997/ipc-jsonrpc/issues"
  },
  "homepage": "https://github.com/gnana997/ipc-jsonrpc#readme",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "biome check .",
    "lint:fix": "biome check --apply .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "npm run build && changeset publish"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.27.10",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^2.1.8",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
